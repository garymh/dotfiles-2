"pamixer --toggle-mute; if `pamixer --get-mute`; then volnoti-show -m; else volnoti-show `pamixer --get-volume`; fi"
  XF86AudioMute

"pamixer --decrease 5; volnoti-show `pamixer --get-volume`"
  XF86AudioLowerVolume

"pamixer --increase 5; volnoti-show `pamixer --get-volume`"
  XF86AudioRaiseVolume

"xbacklight -dec 1"
  XF86MonBrightnessDown

"xbacklight -inc 1"
  XF86MonBrightnessUp

"playerctl play-pause"
  XF86AudioPlay

"playerctl previous"
  XF86AudioPrev

"playerctl next"
  XF86AudioNext

"rofi -combi-modi window,run,ssh -mesg -show combi -modi combi"
  XF86Search
