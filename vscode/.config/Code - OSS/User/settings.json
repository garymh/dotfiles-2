{
    "latex.forwardSearch.executable": "zathura",
    "search.quickOpen.includeHistory": true,
    "search.quickOpen.includeSymbols": true,
    "task.quickOpen.skip": true,
    "latex.forwardSearch.args": ["--synctex-forward", "%l:1:%f", "%p"],
    "latex.server.autoDownload": false,
    "latex.build.onSave": true,
    "workbench.colorTheme": "Base16 Light Grayscale",
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "vscode-neovim.useCtrlKeysForNormalMode": false,
    "window.menuBarVisibility": "toggle",
    "editor.scrollBeyondLastLine": false,
    "editor.fontFamily": "'Fira Code Retina', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "git.alwaysShowStagedChangesResourceGroup": true,
    "git.openDiffOnClick": true,
    "git.showProgress": true,
    "git.showPushSuccessNotification": true,
    "editor.tabSize": 2,
    "editor.fontSize": 12,
    "editor.wordWrapColumn": 100,
    "editor.wordWrap": "wordWrapColumn",
    "editor.minimap.enabled": false,
    "editor.tabCompletion": "on",
    "C_Cpp.updateChannel": "Insiders",
    "mesonbuild.configureOnOpen": true,
    "ccls.completion.caseSensitivity": 1,
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.clang_format_fallbackStyle": "Chromium",
    "C_Cpp.formatting": "Disabled",
    "C_Cpp.errorSquiggles": "Enabled",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "editor.lineNumbers": "relative",
    "editor.fontLigatures": true,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "ccls.codeLens.renderInline": true,
    "ccls.completion.detailedLabel": true,
    "ccls.completion.enableSnippetInsertion": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "explorer",
    "gitlens.views.lineHistory.location": "explorer",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "[latex]": {
        "editor.defaultFormatter": "nickfode.latex-formatter"
    },
}