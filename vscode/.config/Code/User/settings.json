{
  "editor.fontFamily": "'Fira Code Retina','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 100,
  "editor.fontLigatures": true,
  "editor.lineNumbers": "relative",
  "window.menuBarVisibility": "toggle",
  "python.jediEnabled": false,
  "vim.neovimPath": "nvim",
  "vim.visualstar": true,
  "vim.useCtrlKeys": true,
  "vim.statusBarColorControl": false,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.surround": true,
  "vim.leader": "<space>",
  "vim.whichwrap": "h,l,<,>,[,]",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-o>": false,
    "<C-k>": false,
    "<C-b>": false,
    "<C-w>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "g", "p"],
      "commands": ["git.push"]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": ["git.pull"]
    },
    {
      "before": ["<leader>", "t", "h"],
      "commands": [":nohl"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.textwidth": 100,
  "latex-workshop.chktex.enabled": true,
  "latex-workshop.intellisense.surroundCommand.enabled": true,
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.view.pdf.external.command": {
    "command": "zathura",
    "args": ["%PDF%"]
  },
  "latex-workshop.view.pdf.external.synctex": {
    "command": "zathura",
    "args": ["--synctex-forward %LINE%:1:%TEX% %PDF%"]
  },
  "latex-workshop.view.pdf.viewer": "external",
  "editor.wordWrap": "wordWrapColumn",
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 20,
  "editor.detectIndentation": false,
  "workbench.editor.showTabs": true,
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "scm.diffDecorations": "gutter",
  "editor.overviewRulerLanes": 1,
  "scm.diffDecorationsGutterWidth": 1,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "workbench.colorTheme": "Gruvbox Dark (Soft)",
  "org.todoKeywords": ["TODO", "WAITING", "IN-PROGRESS", "DONE", "CANCELED"],
  "python.formatting.provider": "yapf",
  "git.confirmSync": false,
  "ccls.completion.enableSnippetInsertion": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.enabled": true,
  "gitlens.settings.mode": "advanced",
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.hovers.annotations.changes": false,
  "gitlens.hovers.annotations.details": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.currentLine.details": false,
  "gitlens.hovers.currentLine.enabled": false,
  "ccls.completion.detailedLabel": true,
  "ccls.highlighting.bold.enums": false,
  "ccls.highlighting.bold.globalVariables": true,
  "ccls.highlighting.bold.macros": true,
  "ccls.highlighting.italic.types": true,
  "ccls.highlighting.underline.namespaces": true,
  "editor.codeLens": false,
  "C_Cpp.autocomplete": "Disabled",
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.configurationWarnings": "Disabled",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.intelliSenseMode": "clang-x64",
  "C_Cpp.default.includePath": [
    "/usr/include",
    "/usr/include/**",
    "/usr/local/include",
    "/usr/local/include/**"
  ],
  "workbench.iconTheme": "material-icon-theme",
  "vim.enableNeovim": true,
  "rewrap.autoWrap.enabled": true,
  "rewrap.wrappingColumn": 100,
  "ros.distro": "melodic",
  "rewrap.reformat": true,
  "vim.easymotion": true,
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#333333",
    "panel.background": "#555555",
    "sideBar.background": "#444444"
  },
  "path-intellisense.absolutePathToWorkspace": false,
  "explorer.confirmDelete": false
}
