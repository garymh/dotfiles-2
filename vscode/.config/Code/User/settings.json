{
  "editor.fontFamily": "'Fira Code Retina','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 100,
  "editor.fontLigatures": true,
  "editor.lineNumbers": "relative",
  "window.menuBarVisibility": "toggle",
  "python.jediEnabled": false,
  "vim.neovimPath": "nvim",
  "vim.visualstar": true,
  "vim.useCtrlKeys": true,
  "vim.statusBarColorControl": false,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.surround": true,
  "vim.leader": "<space>",
  "vim.whichwrap": "h,l,<,>,[,]",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-o>": false,
    "<C-k>": false,
    "<C-b>": false,
    "<C-w>": false,
    "<C-r>": false,
    "<C-y>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        ":w"
      ]
    },
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "<leader>"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "p"
      ],
      "commands": [
        "git.push"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "l"
      ],
      "commands": [
        "git.pull"
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "h"
      ],
      "commands": [
        ":nohl"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "vim.textwidth": 100,
  "latex-workshop.chktex.enabled": true,
  "latex-workshop.intellisense.surroundCommand.enabled": true,
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.view.pdf.external.command": {
    "command": "zathura",
    "args": [
      "%PDF%"
    ]
  },
  "latex-workshop.view.pdf.external.synctex": {
    "command": "zathura",
    "args": [
      "--synctex-forward %LINE%:1:%TEX% %PDF%"
    ]
  },
  "latex-workshop.view.pdf.viewer": "external",
  "editor.wordWrap": "wordWrapColumn",
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 20,
  "editor.detectIndentation": false,
  "workbench.editor.showTabs": true,
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "scm.diffDecorations": "gutter",
  "editor.overviewRulerLanes": 1,
  "scm.diffDecorationsGutterWidth": 1,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "workbench.colorTheme": "Base16 Gruvbox dark, soft",
  "org.todoKeywords": [
    "TODO",
    "WAITING",
    "IN-PROGRESS",
    "DONE",
    "CANCELED"
  ],
  "python.formatting.provider": "yapf",
  "git.confirmSync": false,
  "ccls.completion.enableSnippetInsertion": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.enabled": true,
  "gitlens.settings.mode": "advanced",
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.hovers.annotations.changes": false,
  "gitlens.hovers.annotations.details": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.currentLine.details": false,
  "gitlens.hovers.currentLine.enabled": false,
  "ccls.completion.detailedLabel": true,
  "ccls.highlighting.bold.enums": false,
  "ccls.highlighting.bold.globalVariables": true,
  "ccls.highlighting.bold.macros": true,
  "ccls.highlighting.italic.types": true,
  "ccls.highlighting.underline.namespaces": true,
  "editor.codeLens": false,
  "C_Cpp.autocomplete": "Disabled",
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.configurationWarnings": "Disabled",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.intelliSenseMode": "clang-x64",
  "C_Cpp.default.includePath": [
    "/usr/include",
    "/usr/include/**",
    "/usr/local/include",
    "/usr/local/include/**"
  ],
  "workbench.iconTheme": "material-icon-theme",
  "vim.enableNeovim": true,
  "rewrap.autoWrap.enabled": true,
  "rewrap.wrappingColumn": 100,
  "ros.distro": "melodic",
  "rewrap.reformat": true,
  "vim.easymotion": true,
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#222222",
    "panel.background": "#222222",
    "activityBar.background": "#414141",
    "sideBar.background": "#444444",
    "terminal.background": "#32302F",
    "terminal.foreground": "#D5C4A1",
    "terminalCursor.background": "#D5C4A1",
    "terminalCursor.foreground": "#D5C4A1",
    "terminal.ansiBlack": "#32302F",
    "terminal.ansiBlue": "#83A598",
    "terminal.ansiBrightBlack": "#665C54",
    "terminal.ansiBrightBlue": "#83A598",
    "terminal.ansiBrightCyan": "#8EC07C",
    "terminal.ansiBrightGreen": "#B8BB26",
    "terminal.ansiBrightMagenta": "#D3869B",
    "terminal.ansiBrightRed": "#FB4934",
    "terminal.ansiBrightWhite": "#FBF1C7",
    "terminal.ansiBrightYellow": "#FABD2F",
    "terminal.ansiCyan": "#8EC07C",
    "terminal.ansiGreen": "#B8BB26",
    "terminal.ansiMagenta": "#D3869B",
    "terminal.ansiRed": "#FB4934",
    "terminal.ansiWhite": "#D5C4A1",
    "terminal.ansiYellow": "#FABD2F"
  },
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.intellisense.unimathsymbols.enabled": true,
  "latex-workshop.synctex.path": "/usr/bin/synctex",
  "path-intellisense.absolutePathToWorkspace": false,
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.sdf": "xml",
    "*.urdf": "xml"
  },
  "lua.format.lineWidth": 100,
  "cpplint.cpplintPath": "/usr/bin/cpplint",
  "cpplint.lineLength": 100
}